{
    "name": "eliaskorolev/menu-builder",
    "description": "drag and drop menu generator like wordpress for laravel 5+",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "authors": [
        {
            "name": "Ilyas Korolev",
            "email": "my@ilyaskorolev.com",
            "role" : "Developer"
        },
    	{
            "name": "Rendy Harimayco",
            "email": "rendyharimayco@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Cristian Garcia",
            "email": "informacion@cristiangarcia.co",
            "role" : "Original Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "5.* || 6.* || 7.* || 8.* || 9.* || 10.*"
    },
    "autoload": {
        "psr-4": {
          "Eliaskorolev\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eliaskorolev\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Eliaskorolev\\Menu\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
